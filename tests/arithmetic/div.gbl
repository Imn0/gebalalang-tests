PROGRAM IS
    a, b, c, d, a_, b_
BEGIN
    READ a ;
    READ b ;
    READ a_;
    READ b_;
    c := a / b;
    d := a % b;
    WRITE c;
    WRITE d;
    c := a_ / b;
    d := a_ % b;
    WRITE c;
    WRITE d;
    c := a / b_;
    d := a % b_;
    WRITE c;
    WRITE d;
    c := a_ / b_;
    d := a_ % b_;
    WRITE c;
    WRITE d;
END